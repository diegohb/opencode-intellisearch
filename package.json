{
  "name": "opencode-intellisearch",
  "version": "1.0.0",
  "description": "Intelligent web search routing for OpenCode with automatic tool selection, graceful fallbacks, and memory caching",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "intellisearch": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc && node dist/scripts/build.js",
    "prepack": "tsc && node dist/scripts/build.js",
    "postinstall": "node dist/bin/cli.js install"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "scripts/**/*",
    "index.ts",
    "tsconfig.json",
    "LICENSE",
    "README.md",
    "INSTALLATION.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "keywords": [
    "opencode",
    "extension",
    "skill",
    "command",
    "search",
    "intellisearch",
    "exa",
    "deepwiki",
    "duckduckgo",
    "bun",
    "bunx",
    "npm",
    "npx"
  ],
  "author": "intellisearch contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/diegohb/opencode-intellisearch"
  },
  "bugs": {
    "url": "https://github.com/diegohb/opencode-intellisearch/issues"
  },
  "homepage": "https://github.com/diegohb/opencode-intellisearch#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "devDependencies": {
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  }
}
